    "numbers = [10, 20, 30, 40, 50]\n",
    "def average_calc(list):\n",
    "    average = 0\n",
    "    for num in list:\n",
    "        average += num\n",
    "    average /= len(list)\n",
    "    return int(average)\n",
    "print(f\"The average value for the list numbers is {average_calc(numbers)}.\")"
   ]
